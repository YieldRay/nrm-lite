{
    "name": "nrm-lite",
    "version": "1.0.0",
    "type": "module",
    "description": "simple and lightweight replacement for nrm",
    "main": "index.mjs",
    "scripts": {
        "test": "node --test",
        "watch": "tsc --watch",
        "format": "prettier --write ."
    },
