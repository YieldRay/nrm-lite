    "repository": {
        "type": "git",
        "url": "https://github.com/YieldRay/nrm-lite.git"
    },
    "bugs": {
        "url": "https://github.com/YieldRay/nrm-lite/issues"
    },
    "homepage": "https://github.com/YieldRay/nrm-lite#readme"
